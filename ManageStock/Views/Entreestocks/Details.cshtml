@model ManageStock.Models.Entreestock

@{
    ViewData["Title"] = "Details de l' Entrée stock";
}


<div>
    <h4>Détails de l' Entrée stock</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateEntree)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateEntree)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdProduitNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdProduitNavigation.Nom)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Quantité)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Quantité)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdFournisseurNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdFournisseurNavigation.Nom)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdEntrepotNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdEntrepotNavigation.Nom)
        </dd>
        
       
    </dl>
</div>
<div>

    <a asp-action="Edit" asp-route-id="@Model?.IdEntree" class="btn btn-warning">Modif.</a>
    <a asp-action="Index" class="btn btn-secondary">Retour</a>
</div>
